<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
		<Console name="Console">
			<patternLayout pattern="%d{HH:mm:ss.SSS} %-5level %c{1} -%msg%n"/>
		</Console>
	</Appenders>
	<Loggers>
	<Root level= "trace">
		<AppenderRef info = "Console"/>
		</Root>
	</Loggers>
</Configuration>